<a name="comparetables"></a>

  <section id="newgs-tables">
    <div class="container">
      <div class="title">
        <h2>
            <small>COMPARE</small>Premium vs Standard
          </h2>
        <p>We have hardware for every budget with no performance compromises. If you need the ultimate power for your power-hungry modded mayhem, we can handle that too. All comparison data was gathered from publicly available information.</p>
      </div>
      <div class="compare-table">
        <!-- New Row -->
        <div class="compare-table-row hidemobile">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1"></div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col">
                  <h4 class="blue-text">Premium</h4>
                </div>
                <div class="col">
                  <h4 class="standard-color">Standard</h4>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row light-bg">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">CPU/Processors</strong>
              <span class="block">How fast your server changes to the game process.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pts">
                  <span class="showmobile blue-text">Performance</span>
                  <img src="/images/newgs-ryzen.webp" class="newgs-cpu" />
                  <strong>AMD Ryzen 9 7950X or Equivalent</strong>
                </div>
                <div class="col pts">
                  <span class="showmobile">Standard</span>
                  <img src="/images/newgs-ryzen.webp" class="newgs-cpu" /> 
                  <span class="block">AMD Ryzen 9 5950X or Equivalent</span>
                </div>
              </div>
            </div>
          </div>
          <div class="flex-grid one compare-speeds-div first-compare">
            <div class="col">
              <a href="javascript:void(0)" class="compare-speeds c-speeds-1">Compare Speeds <img src="/images/newgs-arrow-down.webp" />
                </a>
              <div class="the-charts the-charts-all c-speeds-1-chart">
                <canvas id="bar-chart-cpu" width="800" height="450"></canvas>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">Memory</strong>
              <span class="block">Holds the data that is regularly used to keep track of all the monsters you kill.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pt">
                  <span class="showmobile blue-text">Performance</span>
                  <strong class="block pts">DDR4 ECC 3400 MHz</strong>
                </div>
                <div class="col pt">
                  <span class="showmobile">Standard</span> 
                  <span class="block pts">DDR4 3200 MHz</span>
                </div>
              </div>
            </div>
          </div>
          <div class="flex-grid one compare-speeds-div">
            <div class="col">
              <a href="javascript:void(0)" class="compare-speeds c-speeds-2">Compare Speeds <img src="/images/newgs-arrow-down.webp" />
                </a>
              <div class="the-charts the-charts-all c-speeds-2-chart">
                <canvas id="bar-chart-ram" width="800" height="450"></canvas>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row light-bg">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">SSD</strong>
              <span class="block">Controls how fast the large protions of your games data loads.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pt">
                  <span class="showmobile blue-text">Performance</span>
                  <strong>Samsung NVMe (5100 MB/s)</strong>
                </div>
                <div class="col pt">
                  <span class="showmobile">Standard</span> Enterprise SSD (4300 MB/s)
                </div>
              </div>
            </div>
          </div>
          <div class="flex-grid one compare-speeds-div">
            <div class="col">
              <a href="javascript:void(0)" class="compare-speeds c-speeds-3">Compare Speeds <img src="/images/newgs-arrow-down.webp" />
                </a>
              <div class="the-charts the-charts-all c-speeds-3-chart">
                <canvas id="bar-chart-ssd" width="800" height="450"></canvas>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">Player Slots</strong>
              <span class="block">Max players allowed in your server.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pts">
                  <span class="showmobile blue-text">Performance</span>
                  <strong>Unlimited</strong>
                </div>
                <div class="col pts">
                  <span class="showmobile">Standard</span>
                  <span class="block">Unlimited</span>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row light-bg">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">Cloud Backups</strong>
              <span class="block">Number of backups you can create per server.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pts">
                  <span class="showmobile blue-text">Performance</span>
                  <strong class="block">4</strong>
                </div>
                <div class="col">
                  <span class="showmobile">Standard</span>
                  <span class="block">Up to 2</span>
                  <strong class="blue-text">+$2.00/mo per 2 additional backups</strong>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- New Row -->
        <div class="compare-table-row">
          <div class="flex-grid">
            <div class="col" id="newgs-table-col-1">
              <strong class="block">Locations</strong>
              <span class="block">Worldwide sites to house your game server.</span>
            </div>
            <div class="col" id="newgs-table-col-2">
              <div class="flex-grid two">
                <div class="col pts">
                  <span class="showmobile blue-text">Performance</span>
                  <strong>5 Locations</strong>
                </div>
                <div class="col pts">
                  <span class="showmobile">Budget</span> 4 Locations
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.9.1/chart.min.js" type="text/javascript"></script>
  <script type="text/javascript">
    // CPU chart
    new Chart(document.getElementById("bar-chart-cpu"), {
      type: 'bar',
      data: {
        labels: ["AleForge Premium", "AleForge Standard", "Bisect Hosting", "Streamline Servers", "Nitrado", "HostHavoc"],
        datasets: [{
          label: "CPU Score (Single Core)",
