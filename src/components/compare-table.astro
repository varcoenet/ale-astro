          borderWidth: 1,
          borderRadius: 3,
          data: [3400, 3200, 2933, 2667, 2666, 2667]
        }]
      },
      options: {
        indexAxis: 'y',
        plugins: {
          legend: {
            labels: {
              boxWidth: 0,
              color: "#12161c",
              font: {
                size: 18,
                family: "Metropolis-Bold",
              }
            },
            onClick: null
          }
        },
        title: {
          display: false,
          text: 'CPU Score'
        }
      },
      responsive: true
    });
    // SSD chart
    new Chart(document.getElementById("bar-chart-ssd"), {
      type: 'bar',
      data: {
        labels: ["AleForge Premium", "AleForge Standard", "Bisect Hosting", "Streamline Servers", "Nitrado", "HostHavoc"],
        datasets: [{
          label: "Read/Write Speed",
          backgroundColor: ["rgba(72, 158, 243, 0.25)", "rgba(109, 116, 134, 0.25)", "rgba(3, 221, 255, 0.25)", "rgba(140, 198, 63, 0.25)", "rgba(255, 215, 68, 0.25)", "rgba(254, 153, 0, 0.25)"],
          borderColor: ["rgba(72, 158, 243, 1)", "rgba(109, 116, 134, 1)", "rgba(3, 221, 255, 1)", "rgba(140, 198, 63, 1)", "rgba(255, 215, 68, 1)", "rgba(254, 153, 0, 1)"],
          borderWidth: 1,
          borderRadius: 3,
          data: [5100, 4300, 3500, 3500, 2300, 3500]
        }]
      },
      options: {
        indexAxis: 'y',
        plugins: {
          legend: {
            labels: {
              boxWidth: 0,
              color: "#12161c",
              font: {
                size: 18,
                family: "Metropolis-Bold",
              }
            },
            onClick: null
          }
        },
        title: {
          display: false,
          text: 'CPU Score'
        }
      },
      responsive: true
    });

    $(".c-speeds-1").click(function() {
      $('.compare-speeds').removeClass('opened-speed')
      $(this).toggleClass('opened-speed')
      $(".c-speeds-2-chart").slideUp('fast');
      $(".c-speeds-3-chart").slideUp('fast');
      $(this).parent().find(".the-charts").slideToggle('slow');
    });
    $(".c-speeds-2").click(function() {
      $('.compare-speeds').removeClass('opened-speed')
      $(this).toggleClass('opened-speed')
      $(".c-speeds-1-chart").slideUp('fast');
      $(".c-speeds-3-chart").slideUp('fast');
      $(this).parent().find(".the-charts").slideToggle('slow');
    });
    $(".c-speeds-3").click(function() {
      $('.compare-speeds').removeClass('opened-speed')
      $(this).toggleClass('opened-speed')
      $(".c-speeds-2-chart").slideUp('fast');
      $(".c-speeds-1-chart").slideUp('fast');
      $(this).parent().find(".the-charts").slideToggle('slow');
