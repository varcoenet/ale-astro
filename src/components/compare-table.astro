          borderWidth: 1,
          borderRadius: 3,
          data: [3527, 2923, 2665, 1961, 2440, 2706]
        }]
      },
      options: {
        indexAxis: 'y',
        plugins: {
          legend: {
            labels: {
              boxWidth: 0,
              color: "#12161c",
              font: {
                size: 18,
                family: "Metropolis-Bold",
              }
            },
            onClick: null
          }
        },
        title: {
          display: false,
          text: 'CPU Score'
